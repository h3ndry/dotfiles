[user]
	email = simanga-dev@users.noreply.github.com
	name = Simanga Khoza
[merge]
	tool = nvimdiff
[core]
	editor = nvim
[push]
	default = current
	autoSetupRemote = true
[pull]
	default = matching
	rebase = true
[branch]
	autoSetupMerge = always
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
